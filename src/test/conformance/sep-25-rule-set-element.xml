<?xml version="1.0" encoding="UTF-8"?>
<testcase expect="invalid" xmlns="https://doi.org/10.5281/zenodo.5679629#">
  <label xmlns:testcase="https://doi.org/10.5281/zenodo.5679629#">A sch:rule-set element acts like a pattern w/o the if-then-else logic of rules</label>
  <schemas>
    <sch:schema xmlns:sch="http://purl.oclc.org/dsdl/schematron">
      <sch:rule-set>
        <sch:rule context="*">
          <sch:assert test="true()"/>
        </sch:rule>
        <sch:rule context="*">
          <sch:assert test="false()"/>
        </sch:rule>
      </sch:rule-set>
    </sch:schema>
  </schemas>
  <documents>
    <document filename="document.xml">
      <root xmlns=""/>
    </document>
  </documents>
</testcase>
